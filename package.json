{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "formsy-react": "^1.1.2",
    "formsy-react-components": "^1.0.0-beta.2",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "install": "^0.10.2",
    "material-ui": "^0.20.0",
    "npm": "^5.6.0",
    "react": "^16.3.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-tooltip": "^3.5.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "node-sass-chokidar": "0.0.3",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.4.1",
    "react-scripts": "1.0.17",
    "redux-devtools-extension": "^2.13.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "react-scripts start & npm run build-css & npm run watch-css",
    "build-css": "node-sass-chokidar src/ -o src/stylesheets",
    "watch-css":
      "node-sass-chokidar src/ -o src/stylesheets --watch --recursive",
    "eslint": "./node_modules/.bin/eslint src",
    "eslint-check":
      "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "pretty-quick --staged & npm run eslint",
    "lint": "eslint.js"
  }
}
